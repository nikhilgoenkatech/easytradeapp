{
  "name": "easytrade-load-generator",
  "version": "0.1.0",
  "description": "Load generator for easyTrade based on loadgen-core",
  "main": "./build/src/main.js",
  "scripts": {
    "build": "rm -rf ./build & npx tsc",
    "start": "node .",
    "dev": "node -r ts-node/register --env-file=.env ./src/main.ts",
    "format": "prettier -w .",
    "lint": "eslint ./src"
  },
  "dependencies": {
    "@demoability/loadgen-core": "^0.2.7",
    "puppeteer": "^21.6.0",
    "winston": "^3.10.0"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^6.7.0",
    "eslint": "^8.49.0",
    "prettier": "^3.0.3",
    "ts-node": "^10.9.1",
    "typescript": "^5.3.2"
  }
}
